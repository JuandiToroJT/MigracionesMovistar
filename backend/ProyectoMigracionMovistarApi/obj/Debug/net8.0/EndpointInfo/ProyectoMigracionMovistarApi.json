{
  "openapi": "3.0.1",
  "info": {
    "title": "API Migraciones",
    "description": "API para el proyecto de migraciones de Movistar a Tigo o Claro",
    "contact": {
      "name": "Juan Diego Blandon",
      "email": "juand.blandont@gmail.com"
    },
    "license": {
      "name": "Apache 2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0.html"
    },
    "version": "v1"
  },
  "paths": {
    "/migraciones/procesos": {
      "get": {
        "tags": [
          "ProcesoApi"
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "text/plain": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/ProcesoResumen"
                  }
                }
              },
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/ProcesoResumen"
                  }
                }
              },
              "text/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/ProcesoResumen"
                  }
                }
              }
            }
          },
          "400": {
            "description": "Bad Request",
            "content": {
              "text/plain": {
                "schema": {
                  "$ref": "#/components/schemas/MensajeErrorItem"
                }
              },
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/MensajeErrorItem"
                }
              },
              "text/json": {
                "schema": {
                  "$ref": "#/components/schemas/MensajeErrorItem"
                }
              }
            }
          }
        }
      }
    },
    "/migraciones/{usuario}/manual": {
      "post": {
        "tags": [
          "ProcesoApi"
        ],
        "parameters": [
          {
            "name": "usuario",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/DatosMigracionManual"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/DatosMigracionManual"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/DatosMigracionManual"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "text/plain": {
                "schema": {
                  "$ref": "#/components/schemas/RespuestaTransaccion"
                }
              },
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/RespuestaTransaccion"
                }
              },
              "text/json": {
                "schema": {
                  "$ref": "#/components/schemas/RespuestaTransaccion"
                }
              }
            }
          },
          "400": {
            "description": "Bad Request",
            "content": {
              "text/plain": {
                "schema": {
                  "$ref": "#/components/schemas/MensajeErrorItem"
                }
              },
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/MensajeErrorItem"
                }
              },
              "text/json": {
                "schema": {
                  "$ref": "#/components/schemas/MensajeErrorItem"
                }
              }
            }
          }
        }
      }
    },
    "/migraciones/{usuario}/masiva": {
      "post": {
        "tags": [
          "ProcesoApi"
        ],
        "parameters": [
          {
            "name": "usuario",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "text/plain": {
                "schema": {
                  "$ref": "#/components/schemas/RespuestaTransaccion"
                }
              },
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/RespuestaTransaccion"
                }
              },
              "text/json": {
                "schema": {
                  "$ref": "#/components/schemas/RespuestaTransaccion"
                }
              }
            }
          },
          "400": {
            "description": "Bad Request",
            "content": {
              "text/plain": {
                "schema": {
                  "$ref": "#/components/schemas/MensajeErrorItem"
                }
              },
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/MensajeErrorItem"
                }
              },
              "text/json": {
                "schema": {
                  "$ref": "#/components/schemas/MensajeErrorItem"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "DatosMigracionManual": {
        "type": "object",
        "properties": {
          "identificacion": {
            "type": "string",
            "nullable": true
          },
          "numeroTelefono": {
            "type": "string",
            "nullable": true
          },
          "numeroCuenta": {
            "type": "string",
            "nullable": true
          },
          "correo": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "MensajeErrorItem": {
        "type": "object",
        "properties": {
          "codigoError": {
            "type": "string",
            "nullable": true
          },
          "mensajeError": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "ProcesoResumen": {
        "type": "object",
        "properties": {
          "idProceso": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          },
          "estado": {
            "type": "string",
            "nullable": true
          },
          "total": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          },
          "exitosos": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          },
          "errores": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          },
          "duplicados": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          },
          "fecha": {
            "type": "string",
            "format": "date-time",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "RespuestaTransaccion": {
        "type": "object",
        "properties": {
          "url": {
            "type": "string",
            "nullable": true
          },
          "numeroRegistro": {
            "type": "integer",
            "format": "int64",
            "nullable": true
          },
          "notas": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      }
    }
  }
}